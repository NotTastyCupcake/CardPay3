        <Button 
            x:Name ="AccryalListButton" 
            Content="AccryalListButton" 
            HorizontalAlignment="Center" 
            Width="200"
            VerticalAlignment="Center"/>

        <Button 
            x:Name ="Button" 
            Content="1C" 
            HorizontalAlignment="Right" 
            VerticalAlignment="Center" 
            Width="200"
            Margin="0,0,50,0"/>
    </Grid>
